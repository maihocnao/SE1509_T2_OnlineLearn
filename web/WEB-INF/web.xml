<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SubjectListServlet</servlet-name>
        <servlet-class>controller.SubjectListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogDetailServlet</servlet-name>
        <servlet-class>controller.BlogDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostDetailServlet</servlet-name>
        <servlet-class>controller.PostDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveBlogDetailServlet</servlet-name>
        <servlet-class>controller.EditBlogDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveBlogServlet</servlet-name>
        <servlet-class>controller.SaveBlogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewSubject</servlet-name>
        <servlet-class>controller.AddNewSubjectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LessonDetailServlet</servlet-name>
        <servlet-class>controller.LessonDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LessonSaveServlet</servlet-name>
        <servlet-class>controller.LessonSaveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PricePackageServlet</servlet-name>
        <servlet-class>controller.PricePackageController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SavePricePackageServlet</servlet-name>
        <servlet-class>controller.SavePricePackageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuizHandleServlet</servlet-name>
        <servlet-class>controller.QuizHandleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuizFinishServlet</servlet-name>
        <servlet-class>controller.QuizFinishController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportQuizController</servlet-name>
        <servlet-class>controller.ImportQuizController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadTemplteController</servlet-name>
        <servlet-class>controller.DownloadTemplteController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectListServlet</servlet-name>
        <url-pattern>/SubjectListServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>controller.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserProfileServlet</servlet-name>
        <servlet-class>controller.UserProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserListServlet</servlet-name>
        <servlet-class>controller.UserListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserProfileServlet</servlet-name>
        <url-pattern>/user/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserListServlet</servlet-name>
        <url-pattern>/admin/list-user</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegistrationsListController</servlet-name>
        <servlet-class>controller.RegistrationsListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationsListController</servlet-name>
        <url-pattern>/registrationslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogDetailServlet</servlet-name>
        <url-pattern>/blog/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostDetailServlet</servlet-name>
        <url-pattern>/post/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveBlogDetailServlet</servlet-name>
        <url-pattern>/blog/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveBlogServlet</servlet-name>
        <url-pattern>/blog/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewSubject</servlet-name>
        <url-pattern>/subject/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LessonDetailServlet</servlet-name>
        <url-pattern>/lesson/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LessonSaveServlet</servlet-name>
        <url-pattern>/lesson/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PricePackageServlet</servlet-name>
        <url-pattern>/admin/price-package</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SavePricePackageServlet</servlet-name>
        <url-pattern>/admin/pricepackage/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuizHandleServlet</servlet-name>
        <url-pattern>/quiz/do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuizFinishServlet</servlet-name>
        <url-pattern>/quiz/finish</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportQuizController</servlet-name>
        <url-pattern>/quiz/import</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadTemplteController</servlet-name>
        <url-pattern>/quizlet/down-template</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <env-entry>
        <env-entry-name>serverName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>localhost</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>port</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>1433</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>dbName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>SWP</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>username</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>sa</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>123456</env-entry-value>
    </env-entry>
</web-app>
